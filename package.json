{
    "name": "backend",
    "version": "1.41.8",
    "description": "Backend service for HyperChonk",
    "repository": "https://github.com/hyperchonk/backend",
    "author": "HyperChonk",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": "^18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "start": "bun run dist/apps/main.js",
        "dev": "bun --hot run apps/main.ts",
        "dev:watch": "nodemon --watch apps --watch modules --watch config --ext ts --exec \"bun run apps/main.ts\"",
        "dev:docker": "nodemon --watch apps --watch modules --watch config --ext ts --delay 2 --exec \"bun run apps/main.ts\"",
        "build": "tsc -p tsconfig.build.json",
        "cleanup-console": "find dist -type f -name \"*.js\" -exec sed -i '/console\\.time(\\(.*\\))\\;/d;/console\\.timeEnd(\\(.*\\))\\;/d' {} \\;",
        "generate": "graphql-codegen --config codegen.ts",
        "test": "vitest",
        "vitest": "vitest",
        "logs": "ts-node infrastructure/scripts/get-logs.ts",
        "task": "bun run tasks/index.ts",
        "mutation": "sh scripts/run_mutation.sh",
        "generate-sor-test-data": "bun run test/testData/index.ts false",
        "generate-sor-test-data:overwrite": "bun run test/testData/index.ts true",
        "deploy": "scripts/deploy-cli.sh",
        "deploy:build": "scripts/deploy-cli.sh -w build",
        "deploy:build:force": "scripts/deploy-cli.sh -w build -f",
        "deploy:dev": "scripts/deploy-cli.sh -w deploy-code -e dev",
        "deploy:staging": "scripts/deploy-cli.sh -w deploy-code -e staging",
        "deploy:production": "scripts/deploy-cli.sh -w deploy-code -e production -c",
        "deploy:infra:dev": "scripts/deploy-cli.sh -w deploy-infra -e dev",
        "deploy:infra:staging": "scripts/deploy-cli.sh -w deploy-infra -e staging",
        "deploy:infra:production": "scripts/deploy-cli.sh -w deploy-infra -e production -c"
    },
    "dependencies": {
        "@apollo/server": "^4.11.3",
        "@aws-sdk/client-cloudwatch": "^3.734.0",
        "@aws-sdk/client-secrets-manager": "^3.734.0",
        "@aws-sdk/client-sqs": "^3.734.0",
        "@aws-sdk/client-ssm": "^3.849.0",
        "@balancer-labs/balancer-maths": "^0.0.32",
        "@balancer/sdk": "^1.4.1",
        "@ethersproject/address": "^5.7.0",
        "@ethersproject/bignumber": "^5.7.0",
        "@ethersproject/constants": "^5.7.0",
        "@googleapis/sheets": "^9.3.1",
        "@prisma/client": "^6.3.0",
        "@prisma/instrumentation": "^6.3.0",
        "@sanity/client": "^2.23.2",
        "@sentry/integrations": "^7.114.0",
        "@sentry/node": "^9.0.1",
        "@sentry/tracing": "^7.120.3",
        "@types/graphql-fields": "^1.3.9",
        "abitype": "^1.0.8",
        "axios": "^0.24.0",
        "bignumber.js": "^9.1.2",
        "decimal.js": "^10.5.0",
        "dotenv": "^10.0.0",
        "ethers": "^5.7.2",
        "express": "^4.21.2",
        "graphql": "^16.10.0",
        "graphql-fields": "^2.0.3",
        "graphql-request": "^3.7.0",
        "graphql-tag": "^2.12.6",
        "helmet": "^5.1.1",
        "limiter": "^2.1.0",
        "lodash": "^4.17.21",
        "memory-cache": "^0.2.0",
        "moment-timezone": "^0.5.47",
        "murmurhash": "^2.0.1",
        "prisma": "^6.3.0",
        "stellate": "2.7.1",
        "ts-dotenv": "^0.8.4",
        "viem": "^2.31.7"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.12",
        "@faker-js/faker": "^8.4.1",
        "@graphql-codegen/add": "^3.2.3",
        "@graphql-codegen/cli": "^2.16.5",
        "@graphql-codegen/introspection": "^2.2.3",
        "@graphql-codegen/schema-ast": "^2.6.1",
        "@graphql-codegen/typescript": "2.4.0",
        "@graphql-codegen/typescript-document-nodes": "^2.3.13",
        "@graphql-codegen/typescript-graphql-request": "^4.5.9",
        "@graphql-codegen/typescript-operations": "^2.5.13",
        "@graphql-codegen/typescript-resolvers": "2.4.1",
        "@graphql-tools/schema": "^8.5.1",
        "@types/bun": "^1.2.8",
        "@types/lodash": "^4.17.15",
        "@types/memory-cache": "^0.2.6",
        "@types/node": "^18.19.74",
        "@viem/anvil": "^0.0.10",
        "execa": "7.1.1",
        "fishery": "^2.2.2",
        "msw": "^0.47.4",
        "nodemon": "^3.0.2",
        "prettier": "^2.8.8",
        "testcontainers": "^8.16.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "vitest": "^3.0.8",
        "vitest-mock-extended": "^1.3.2"
    }
}
